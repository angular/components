{
  "name": "harness-e2e-cli",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run-e2e-specs": "node --loader ts-node/esm node_modules/jasmine/bin/jasmine --config=e2e/jasmine.json",
    "wait-and-run-e2e": "wait-on http://localhost:4200 && pnpm run-e2e-specs",
    "e2e": "concurrently -s first -k 'pnpm ng serve' 'pnpm wait-and-run-e2e'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.0-rc.1",
    "@angular/cdk": "next",
    "@angular/common": "20.0.0-rc.1",
    "@angular/compiler": "20.0.0-rc.1",
    "@angular/core": "20.0.0-rc.1",
    "@angular/forms": "20.0.0-rc.1",
    "@angular/material": "next",
    "@angular/platform-browser": "20.0.0-rc.1",
    "@angular/router": "20.0.0-rc.1",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.0-rc.2",
    "@angular/cli": "20.0.0-rc.2",
    "@angular/compiler-cli": "20.0.0-rc.1",
    "@types/jasmine": "5.1.7",
    "@types/node": "22.14.0",
    "@types/selenium-webdriver": "3.0.19",
    "concurrently": "^9.0.0",
    "jasmine": "5.6.0",
    "jasmine-core": "5.6.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "selenium-webdriver": "3.6.0",
    "ts-node": "~10.9.1",
    "typescript": "5.8.3",
    "wait-on": "^8.0.0"
  }
}
