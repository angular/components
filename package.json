    "rollup": "^4.0.0",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-sourcemaps2": "0.5.3",
    "sass": "^1.80.6",
    "selenium-webdriver": "^3.6.0",
    "semver": "^7.3.5",
    "shelljs": "^0.10.0",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "stylelint": "^14.14.0",
    "terser": "^5.10.0",
    "ts-node": "^10.9.1",
    "tsec": "0.2.8",
    "tsickle": "0.46.3",
    "tslint": "^6.1.3",
    "tsutils": "^3.21.0",
    "typescript": "5.9.2",
    "vrsource-tslint-rules": "6.0.0",
    "yargs": "^18.0.0",
    "zx": "^8.0.0"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.9.2"
    },
    "onlyBuiltDependencies": [],
    "packageExtensions": {
      "dgeni-packages": {
        "dependencies": {
          "clonedeep": "*"
        }
      },
      "grpc-gcp": {
        "dependencies": {
          "protobufjs": "7.4.0"
        }
      },
      "@angular-eslint/eslint-plugin-template@19.3.0": {
        "peerDependencies": {
          "@typescript-eslint/parser": "*",
          "@angular-eslint/template-parser": "*"
        }
      },
      "@angular/animations": {
        "dependencies": {
          "@nginfra/angular-linking": "1.0.9"
        },
