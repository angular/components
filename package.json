    "@octokit/rest": "22.0.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@schematics/angular": "catalog:",
    "@types/babel__core": "^7.1.18",
    "@types/fs-extra": "^11.0.0",
    "@types/jasmine": "^5.0.0",
    "@types/luxon": "^3.0.0",
    "@types/node": "^22.14.1",
    "@types/selenium-webdriver": "^3.0.17",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.8",
    "autoprefixer": "^10.4.2",
    "axe-core": "^4.10.3",
    "chalk": "^5.4.1",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.30.0",
    "esbuild": "^0.25.0",
    "firebase-tools": "14.12.0",
    "fs-extra": "^11.0.0",
    "glob": "^11.0.3",
    "highlight.js": "^11.0.0",
    "husky": "^9.0.1",
    "jasmine": "^5.6.0",
    "jasmine-core": "5.9.0",
    "jasmine-reporters": "^2.5.2",
    "jsonc-parser": "^3.3.1",
    "kagekiri": "^2.0.0",
    "karma": "^6.3.12",
