{
  "name": "material-angular-io",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "npm run build-themes && ng serve",
    "start-jit": "npm run build-themes && ng serve --aot=false",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "fetch-local": "bash ./tools/fetch-assets-local.sh",
    "build-themes": "bash ./tools/build-themes.sh",
    "prod-build": "npm run build-themes && ng build --aot --prod",
    "postinstall": "webdriver-manager update && bash ./tools/build-themes.sh && bash ./tools/fetch-assets.sh",
    "publish-prod": "bash ./tools/deploy.sh stable prod",
    "publish-dev": "bash ./tools/deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0-rc.5",
    "@angular/cdk": "6.0.0-rc.13",
    "@angular/common": "6.0.0-rc.5",
    "@angular/compiler": "6.0.0-rc.5",
    "@angular/core": "6.0.0-rc.5",
    "@angular/forms": "6.0.0-rc.5",
    "@angular/http": "6.0.0-rc.5",
    "@angular/material": "6.0.0-rc.13",
    "@angular/material-moment-adapter": "6.0.0-rc.0",
    "@angular/platform-browser": "6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "6.0.0-rc.5",
    "@angular/router": "6.0.0-rc.5",
    "@angular/service-worker": "6.0.0-rc.5",
    "core-js": "^2.4.1",
    "moment": "^2.19.1",
    "rxjs": "6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.5.8",
    "@angular/cli": "6.0.0-rc.5",
    "@angular/compiler-cli": "6.0.0-rc.5",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.5",
    "firebase-tools": "^3.15.3",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.9.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
